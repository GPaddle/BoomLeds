{
  "name": "panel-painter",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "init": "mkdirp dist",
    "app-client": "cpx \"src/client/*\" dist/",
    "jshint-client": "jshint src/client/*.js",
    "gzip:dirs": "mkdirp dist-gz/addons/",
    "gzip:app": "cat dist/app.js | gzip > dist-gz/app.js.gz",
    "gzip:index": "cat dist/index.html | gzip > dist-gz/index.html.gz",
    "gzip:style": "cat dist/style.css | gzip > dist-gz/style.css.gz",
    "build": "npm-run-all clean jshint-client init app-client",
    "server": "node src/server/server.js",
    "gzips": "npm-run-all gzip:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GPaddle/BoomLeds.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GPaddle/BoomLeds/issues"
  },
  "homepage": "https://github.com/GPaddle/BoomLeds#readme",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "cpx": "^1.5.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "jshint": "^2.10.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0"
  }
}
